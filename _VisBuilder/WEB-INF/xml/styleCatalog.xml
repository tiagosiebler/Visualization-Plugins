<StyleCatalog description="MSTR System Style Catalog" name="MSTRStyleCatalog"
              version="1.1">
    <Styles>
        <Style name="VisBuilderNew" description="VisBuilder New Visualization" transform="ReportAjaxMojoVisualizationTransform">
            <ActualParameters>
                <ActualParameter name="documentStyle" type="1" value="RWGridJsonStyle" />
                <ActualParameter name="reportXMLStyle" type="1" value="VisualizationDataStyle" />
                <ActualParameter name="mojoClassName" type="1" value="plugins._VisBuilder.VisBuilderNew"/>
            </ActualParameters>
            <Layouts>
                <Layout layout_source="AppLayoutSourceFile" order="1">
                    <![CDATA[/WEB-INF/xml/layouts/MojoVisualizationLayout.xml]]>
                </Layout>
            </Layouts>
        </Style>
        <Style description="Style: Display the Toolbar in the Visualization Builder Page" name="VBToolbarStyle" transform="MojoToolbarTransform">
            <Layouts>
                <Layout layout_source="AppLayoutSourceFile" order="1">
                    <![CDATA[@_VisBuilder:/WEB-INF/xml/layouts/blocks/VBToolbarRequestLoader.xml]]>
                </Layout>
            </Layouts>
        </Style>

        <Style
                description="Style: Display the Toolbar in the Visualization Builder  Page"
                name="VBMenubarStyle" transform="MojoToolbarTransform">
            <Layouts>
                <Layout layout_source="AppLayoutSourceFile" order="1">
                    <![CDATA[@_VisBuilder:/WEB-INF/xml/layouts/blocks/VBMenubarRequestLoader.xml]]>
                </Layout>
            </Layouts>
        </Style>
    </Styles>
</StyleCatalog>
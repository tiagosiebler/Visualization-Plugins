<!DOCTYPE block SYSTEM "../dtds/blockLibrary.dtd">
<block name="VBMenubarModel">
    <definition>
        <!-- An array of buttons. -->
        <property name="items" type="List" />
    </definition>
    <assignments>
        <property name="items">
            <list>
                <block name="VBMenubarFileModel" />
                <block name="VBMenubarViewModel" />
